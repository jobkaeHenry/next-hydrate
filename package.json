{
  "name": "@jobkaehenry/next-hydrate",
  "version": "0.1.0",
  "description": "Universal hydration utilities for Next.js App Router and React Query v5",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm,cjs",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "dev:test": "vitest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "nextjs",
    "react-query",
    "hydration",
    "ssr",
    "isr",
    "ssg"
  ],
  "author": "jobkaehenry",
  "license": "MIT",
  "peerDependencies": {
    "@tanstack/react-query": "^5.0.0",
    "next": ">=13.4.0",
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0"
  },
  "dependencies": {
    "@tanstack/react-query-devtools": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@vitest/coverage-v8": "^1.6.0",
    "tsup": "^7.2.0",
    "typescript": "^5.3.0",
    "vitest": "^1.6.0"
  }
}
